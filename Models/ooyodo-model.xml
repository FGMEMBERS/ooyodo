<!-- rudder -->
  <animation>
    <type>rotate</type>
    <object-name>rudder</object-name>
    <property>controls/flight/aileron</property>
    <factor> 15 </factor>
    <center>
      <x-m> 83.2 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.00000 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

<!-- Launch from the catapult -->
  <animation>
    <type>select</type>
    <object-name>E13A1-2</object-name>
    <condition>
      <not>
	<equals>
		<property>controls/armament/station[0]/E13A1</property>
		<value>1</value>
	</equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>E13A1-2</object-name>
    <condition>
      <not>
	<equals>
		<property>controls/armament/station[1]/E13A1</property>
		<value>1</value>
	</equals>
      </not>
    </condition>
  </animation>

<!-- catapult --> 	
  <animation>
  <type>rotate</type>
    <object-name>Catapult</object-name>
    <object-name>Rturn</object-name>
    <object-name>E13A1-2</object-name>
    <property>controls/flight/rudder</property>
    <factor> 60 </factor>
    <center>
      <x-m>69.6</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

<!-- terret --> 	
  <animation>
  <type>rotate</type>
    <object-name>Sturn</object-name>
    <object-name>Sterret</object-name>
    <object-name>Scover-l</object-name>
    <object-name>Scover-c</object-name>
    <object-name>Scover-r</object-name>
    <object-name>Scover-l.001</object-name>
    <object-name>Scover-c.001</object-name>
    <object-name>Scover-r.001</object-name>
    <object-name>Sbarrel-l</object-name>
    <object-name>Sbarrel-c</object-name>
    <object-name>Sbarrel-r</object-name>
    <object-name>Cylinder.029</object-name>
    <object-name>Cylinder.030</object-name>
    <object-name>Cylinder.031</object-name>
    <object-name>Cube.005</object-name>
    <property>/controls/flight/elevator</property>
    <factor> 120 </factor>
    <center>
      <!--<x-m>-43.0</x-m>-->
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
  <type>rotate</type>
    <object-name>Scover-l</object-name>
    <object-name>Scover-c</object-name>
    <object-name>Scover-r</object-name>
    <object-name>Sbarrel-l</object-name>
    <object-name>Sbarrel-c</object-name>
    <object-name>Sbarrel-r</object-name>
    <property>/controls/flight/flaps</property>
    <factor> 45 </factor>
    <center>
      <!--<x-m>-46.0</x-m>
