<!-- screw -->
  <animation>
    <type>spin</type>
    <object-name>screw-lo</object-name>
    <property>controls/flight/throttle-filtered</property>
    <factor> 120 </factor>
    <center>
      <x-m>  0.0 </x-m>
      <y-m> -5.10 </y-m>
      <z-m> -3.573 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>screw-li</object-name>
    <property>controls/flight/throttle-filtered</property>
    <factor> 120 </factor>
    <center>
      <x-m>  0.0 </x-m>
      <y-m> -2.30 </y-m>
      <z-m> -3.573 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>screw-ri</object-name>
    <property>controls/flight/throttle-filtered</property>
    <factor> 120 </factor>aa
    <center>
      <x-m>  0.0 </x-m>
      <y-m>  2.30 </y-m>
      <z-m> -3.573 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
